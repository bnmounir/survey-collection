        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && yarn install && npm run build"
    },
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.0",
        "concurrently": "^5.1.0",
        "cookie-session": "^1.4.0",
        "express": "^4.17.1",
        "mongoose": "^5.8.10",
        "nodemon": "^2.0.2",
        "passport": "^0.4.1",
        "passport-google-oauth20": "2",
        "stripe": "^8.14.0"
    }
}
