{
    "name": "server",
    "version": "1.0.0",
    "main": "index.js",
    "engines": {
        "node": "13.1.0",
        "npm": "6.13.6",
        "yarn": "1.21.1"
    },
    "scripts": {
        "start": "node index.js",
        "server": "nodemon index.js",
        "client": "cd client && yarn run start",
